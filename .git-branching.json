{
    "root": {
        "allowed": [],
        "disallowed": ["master", "develop"]
    },
    "flow": {
        "separator": "/",
        "prefix": ["feature", "fix", "hotfix", "release"],
        "suffix": {
            "regex": "(SPARAC\\-\\d{1,5})"
        },
        "range": [7, 18]
    },
    "scripts": [
        ["Console cache:clear", ["bin/console", ["c:c"]]],
        ["Phpunit tests", ["bin/phpunit", []]],
        ["Behat tests", ["bin/behat", []]]
    ],
    "skipMerge": true
}
