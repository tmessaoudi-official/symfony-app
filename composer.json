{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Takieddine Message",
            "email": "tmessaoudi@smart-team.tn"
        }
    ],
    "prefer-stable": false,
    "require": {
        "php": ">=8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-json": "*",
        "api-platform/api-pack": "1.*",
        "api-platform/core": "2.6.x-dev",
        "composer/package-versions-deprecated": "1.*",
        "doctrine/annotations": "1.12.x-dev",
        "doctrine/doctrine-bundle": "dev-master",
        "doctrine/doctrine-migrations-bundle": "3.*",
        "doctrine/inflector": "1.4.*",
        "doctrine/orm": "2.*",
        "doctrine/persistence": "2.2.x-dev",
        "gesdinet/jwt-refresh-token-bundle": "0.*",
        "lcobucci/jwt": "~3.2.5",
        "lexik/jwt-authentication-bundle": "2.*",
        "ramsey/uuid-doctrine": "1.*",
        "spomky-labs/lexik-jose-bridge": "^3.0",
        "symfony/apache-pack": "^1.0",
        "symfony/cache": "5.2.*",
        "symfony/cache-contracts": "2.*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/flex": "1.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/maker-bundle": "1.*",
        "symfony/monolog-bundle": "3.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/serializer-pack": "1.*",
        "symfony/yaml": "5.2.*",
        "web-token/jwt-encryption-algorithm-aescbc": "^2.0",
        "web-token/jwt-encryption-algorithm-aesgcm": "^2.0",
        "web-token/jwt-encryption-algorithm-aesgcmkw": "^2.0",
        "web-token/jwt-encryption-algorithm-aeskw": "^2.0",
        "web-token/jwt-encryption-algorithm-dir": "^2.0",
        "web-token/jwt-encryption-algorithm-ecdh-es": "^2.0",
        "web-token/jwt-encryption-algorithm-experimental": "^2.0",
        "web-token/jwt-encryption-algorithm-pbes2": "^2.0",
        "web-token/jwt-encryption-algorithm-rsa": "^2.0",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.0",
        "web-token/jwt-signature-algorithm-eddsa": "^2.0",
        "web-token/jwt-signature-algorithm-experimental": "^2.0",
        "web-token/jwt-signature-algorithm-hmac": "^2.0",
        "web-token/jwt-signature-algorithm-none": "^2.0",
        "web-token/jwt-signature-algorithm-rsa": "^2.0",
        "willdurand/negotiation": "3.*"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behat/debug-extension": "^1.0",
        "behat/mink-goutte-driver": "1.2.x-dev",
        "behat/mink-selenium2-driver": "1.4.x-dev",
        "bex/behat-screenshot": "dev-master",
        "dama/doctrine-test-bundle": "7.0.x-dev",
        "doctrine/doctrine-fixtures-bundle": "3.5.x-dev",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/symfony-extension": "^2.1",
        "friends-of-behat/test-context": "^1.2",
        "friendsofphp/php-cs-fixer": "3.0.x-dev",
        "sensiolabs/security-checker": "^6.0",
        "symfony/debug-pack": "1.*",
        "symfony/profiler-pack": "1.*",
        "symfony/requirements-checker": "2.*",
        "symfony/test-pack": "1.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\PhpUnit\\": "tests/src/PhpUnit/",
            "App\\Tests\\Behat\\": "tests/src/Behat/"
        },
        "files": [
            "override/Bex/Behat/ScreenshotExtension/Service/FilenameGenerator.php"
        ]
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "requirements-checker": "script",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "schema:validate": "./bin/console doctrine:schema:validate --skip-sync",
        "yaml:lint": "./bin/console lint:yaml config/ --ansi",
        "twig:lint": "./bin/console lint:twig templates/ --ansi",
        "cs:check": "./vendor/bin/php-cs-fixer fix --dry-run --verbose --ansi",
        "cs:fix": "./vendor/bin/php-cs-fixer fix --ansi --verbose",
        "check": [
            "@schema:validate",
            "@yaml:lint",
            "@twig:lint",
            "@cs:check"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    }
}
